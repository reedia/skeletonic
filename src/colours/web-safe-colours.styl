  for web-safe-colours, i in web-safe-colours
    // Web Safe Background Colours
    .bg-colour-{i+1}
      background-color: web-safe-colours

    // Web Safe Text Colours
    .colour-{i+1}
      color: web-safe-colours

    // Web Safe Button Colours
    .btn-wsc-{i+1}
      background-color: web-safe-colours
      border-radius: 50%
      cursor: pointer
      display: inline-block
      height: 60px
      line-height: 60px
      overflow: hidden
      padding: 0
      position: relative
      transition: background-color .3s
      vertical-align: middle
      width: 60px
      z-index: 1

